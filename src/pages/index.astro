---
import Layout from "../layouts/Layout.astro";
import TextGradient from "../components/TextGradient.astro";
---

<Layout title="fmedinilla">
	<main class="main">
		<h1 class="main__h1"><TextGradient>fmedinilla</TextGradient></h1>
	</main>
</Layout>

<style>
	.main {
		display: grid;
		place-items: center;
		height: 100%;
	}

	.main__h1 {
		color: #ddd;
		font-size: 5em;
		text-align: center;
		padding: 2rem;
	}
</style>
